{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"globalDependencies": [
		".env"
	],
	"tasks": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"inputs": [
				"$TURBO_DEFAULT$",
				".env*"
			],
			"outputs": [
				".tanstack/start/**",
				"!.tanstack/start/cache/**"
			],
			"env": [
				"NODE_ENV",
				"VERCEL_ENV",
				"VERCEL_URL",
				"VERCEL_GIT_COMMIT_SHA",
				"VERCEL_GIT_COMMIT_REF",
				"VITE_API_KEY",
				"SECRET_KEY"
			],
			"passThroughEnv": [
				"NODE_ENV",
				"VERCEL_ENV",
				"VERCEL_URL",
				"VERCEL_GIT_COMMIT_SHA",
				"VERCEL_GIT_COMMIT_REF",
				"NITRO_PRESET",
				"VITE_API_KEY",
				"SECRET_KEY"
			]
		},
		"lint": {
			"dependsOn": [
				"^lint"
			]
		},
		"check-types": {
			"dependsOn": [
				"^check-types"
			]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"dev:setup": {
			"cache": false,
			"persistent": true
		}
	}
}